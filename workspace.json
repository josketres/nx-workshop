{
  "version": 2,
  "projects": {
    "api": "apps/api",
    "api-util-interface": {
      "root": "libs/api/util-interface",
      "sourceRoot": "libs/api/util-interface/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/util-interface/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/util-interface"],
          "options": {
            "jestConfig": "libs/api/util-interface/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "feature-game-detail": {
      "projectType": "library",
      "root": "libs/feature-game-detail",
      "sourceRoot": "libs/feature-game-detail/src",
      "prefix": "bg-hoard",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/feature-game-detail"],
          "options": {
            "jestConfig": "libs/feature-game-detail/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature-game-detail/src/**/*.ts",
              "libs/feature-game-detail/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "libs-store-ui-shared": {
      "projectType": "library",
      "root": "libs/libs/store/ui-shared",
      "sourceRoot": "libs/libs/store/ui-shared/src",
      "prefix": "bg-hoard",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/libs/store/ui-shared"],
          "options": {
            "jestConfig": "libs/libs/store/ui-shared/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/libs/store/ui-shared/src/**/*.ts",
              "libs/libs/store/ui-shared/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "libs-store-util-formatters": "libs/libs/store/util-formatters",
    "store": "apps/store",
    "store-e2e": "apps/store-e2e"
  }
}

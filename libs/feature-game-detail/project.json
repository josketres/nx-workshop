{
  "projectType": "library",
  "root": "libs/feature-game-detail",
  "sourceRoot": "libs/feature-game-detail/src",
  "prefix": "bg-hoard",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/feature-game-detail"
      ],
      "options": {
        "jestConfig": "libs/feature-game-detail/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/feature-game-detail/src/**/*.ts",
          "libs/feature-game-detail/src/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "type:feature"
  ]
}
